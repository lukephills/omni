{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-duplicate-key": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [true, "always"],
    "trailing-comma": false,
		"triple-equals": true,
		"no-unexternalized-strings": [
			true,
			{
				"signatures": ["localize", "nls.localize"],
				"keyIndex": 0,
				"messageIndex": 1
			}
		]
	}
}